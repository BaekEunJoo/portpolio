<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.Imuseum.qnaBoard.dao.QnaBoardCategoryDAO">
	<select id="getCategory" resultType="qnaCategory">
		SELECT *
		FROM QNA_CATEGORY
		WHERE MENU_IDX = #{menuIdx}
		ORDER BY QUESTION_CATEGORY
	</select>
</mapper>






